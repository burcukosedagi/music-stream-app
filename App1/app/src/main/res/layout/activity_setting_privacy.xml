<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/privacyPage"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="#040404"
tools:context=".SettingPrivacyActivity">


<ImageButton
    android:id="@+id/btnBacksettings"
    android:layout_width="53dp"
    android:layout_height="52dp"
    android:backgroundTint="#040404"
    android:onClick="backtToSetOnClick"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.043"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.024"
    app:srcCompat="?attr/actionModeCloseDrawable"
    tools:ignore="OnClick" />

    <ImageButton
        android:id="@+id/imgBtnPhoto"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:backgroundTint="#121212"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/gridLayout7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.731"
        app:srcCompat="@drawable/rounded_cornercircle" />

    <Button
        android:id="@+id/btnSave"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginStart="138dp"
        android:layout_marginTop="520dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="99dp"
        android:backgroundTint="#2E2D2D"
        android:text="Kaydet"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/txtSignInTime"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.988" />

    <androidx.gridlayout.widget.GridLayout
    android:id="@+id/gridLayout7"
    android:layout_width="288dp"
    android:layout_height="290dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.629"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="245dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="Kullanıcı Adı"
        android:textColor="#D0CECE"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_column="0"
        app:layout_row="0" />

    <EditText
        android:id="@+id/editTxtUserName"
        android:layout_width="245dp"
        android:layout_height="50dp"
        android:backgroundTint="#D0CECE"
        android:ems="10"
        android:inputType="text"
        android:hint="Name"
        android:textColorHint="#575656"
        android:textSize="16sp"
        app:layout_column="0"
        app:layout_row="1" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="245dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="E-posta Adresi"
        android:textColor="#D0CECE"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_column="0"
        app:layout_row="2" />

    <EditText
        android:id="@+id/editTxtTxtEmail"
        android:layout_width="245dp"
        android:layout_height="50dp"
        android:backgroundTint="#D0CECE"
        android:ems="10"
        android:inputType="textPostalAddress"
        android:hint="E-mail"
        android:textColorHint="#575656"
        app:layout_column="0"
        app:layout_row="3" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="245dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="Şifre"
        android:textColor="#D0CECE"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_column="0"
        app:layout_row="4" />

    <EditText
        android:id="@+id/editTxtTxtPassword"
        android:layout_width="245dp"
        android:layout_height="50dp"
        android:backgroundTint="#D0CECE"
        android:ems="10"
        android:hint="Password"
        android:textColorHint="#575656"
        android:inputType="textPassword"
        app:layout_column="0"
        app:layout_row="5" />

</androidx.gridlayout.widget.GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>